  }
  
  #cssmenu:before,
  #cssmenu:after,
  #cssmenu > ul:before,
  #cssmenu > ul:after {
    content: '';
    display: table;
  }
  
  #cssmenu:after,
  #cssmenu > ul:after
  {
    clear: both;
  }
  
  #cssmenu ul {
